
public class Sexo {
	String nombre;

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public Sexo(String nombre) {
		super();
		this.nombre = nombre;
	}

	@Override
	public String toString() {
		return "Sexo [nombre=" + nombre + "]";
	}
	
	
}
